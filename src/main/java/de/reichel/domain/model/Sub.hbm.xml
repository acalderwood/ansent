<class 
    name="de.reichel.domain.model.Sub" 
    table="SUBS"
>

    <id
        name="idSub"
        type="java.lang.Integer"
        column="ID_SUB"
    >
        <generator class="increment" />
    </id>

    <property
        name="subName"
        type="java.lang.String"
        column="SUB_NAME"
        not-null="true"
        length="100"
    />
    <property
        name="strasseNr"
        type="java.lang.String"
        column="STRASSE_NR"
        length="100"
    />
    <property
        name="plz"
        type="java.lang.String"
        column="PLZ"
        length="5"
    />
    <property
        name="ort"
        type="java.lang.String"
        column="ORT"
        length="100"
    />
    <property
        name="tel"
        type="java.lang.String"
        column="TEL"
        length="100"
    />
    <property
        name="fax"
        type="java.lang.String"
        column="FAX"
        length="100"
    />
    <property
        name="timestamp"
        type="java.sql.Timestamp"
        column="TIMESTAMP"
        not-null="true"
        length="19"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
