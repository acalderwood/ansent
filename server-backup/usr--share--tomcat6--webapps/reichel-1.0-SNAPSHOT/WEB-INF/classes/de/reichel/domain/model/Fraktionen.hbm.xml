<class 
    name="de.reichel.domain.model.Fraktionen" 
    table="FRAKTIONEN"
>

    <id
        name="idFraktion"
        type="java.lang.Integer"
        column="ID_FRAKTION"
    >
        <generator class="increment" />
    </id>

    <property
        name="timestamp"
        type="java.sql.Timestamp"
        column="TIMESTAMP"
        not-null="true"
        length="19"
    />
    <property
        name="beschreibung"
        type="java.lang.String"
        column="BESCHREIBUNG"
        not-null="true"
        length="20"
    />
    <property
        name="beschreibungLang"
        type="java.lang.String"
        column="BESCHREIBUNG_LANG"
        length="200"
    />

    <!-- Associations -->
  

</class>
</hibernate-mapping>
